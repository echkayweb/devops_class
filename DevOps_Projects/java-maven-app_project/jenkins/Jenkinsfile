pipeline {
    agent any

    stages{
        stage("Git Checkout"){
            steps{
                git url:"https://github.com/mmukul/devops_class"
            }
        }
  
    stages {
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
